<section id="sign_in">
  <div class="sign_in_inner">
    <h2>Help Us Map Chicago's Historic Water Tanks</h2>
      <div class="sign_in_form">

        <%= form_for(resource, as: resource_name, url: session_path(resource_name), class: "form") do |f| %>
          <div>
            <%= f.email_field :email, placeholder: 'Email Address', autofocus: true, autocomplete: "email" %>
          </div>

          <div>
            <%= f.password_field :password, placeholder: 'Password', autocomplete: "off" %>
          </div>

          <!-- <% if devise_mapping.rememberable? -%>
            <div class="field">
              <%= f.check_box :remember_me %>
              <%= f.label :remember_me %>
            </div>
          <% end -%> -->

          <div>
            <%= f.submit "Sign In", class: "button" %>
          <div>
        <% end %>

    </div>
  </div>
</section>

